<?xml version="1.0" encoding="ISO-8859-1" ?>
<gatein-resources xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.gatein.org/xml/ns/gatein_resources_1_3 http://www.gatein.org/xml/ns/gatein_resources_1_3"
  xmlns="http://www.gatein.org/xml/ns/gatein_resources_1_3">
                  
  <portlet-skin>
    <!-- this is the package name war -->
    <!-- important for css since tomcat uses package name to get resources -->
    <application-name>bookstore</application-name>
    <portlet-name>BookstorePortlet</portlet-name>
    <skin-name>Default</skin-name>
    <css-path>/skin/DefaultSkin/webui/Stylesheet.css</css-path>
    <css-priority>1</css-priority>
  </portlet-skin>                  

  <portlet>
    <name>estudyportlet</name>
    <module>
      <script>
        <path>/javascript/estudy/estudy.js</path>
      </script>
      <depends>
        <module>bts_tooltip</module>
      </depends>
	    <depends>
	      <module>common</module>
	    </depends>
	    <depends>
	      <module>base</module>
	      <as>base</as>
	    </depends>
	    <depends>
	      <module>uiPopup</module>
	    </depends>
	    <depends>
	      <module>navigation</module>
	      <as>gtnav</as>
	    </depends>
	    <depends>
	      <module>commons-cometd</module>
	      <as>cometd</as>
	    </depends>
	    <depends>
	      <module>uiForm</module>
	    </depends>
	    <depends>
	      <module>uiPopupWindow</module>
	    </depends>
	    <depends>
	      <module>webui-ext</module>
	      <as>wx</as>
	    </depends>
	    <depends>
	      <module>jquery</module>
	      <as>gj</as>
	    </depends>
    
    </module>
  </portlet>
</gatein-resources>